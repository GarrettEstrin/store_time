<h1> <%= link_to "Community Story Time", stories_path %> </h1>
<hr>
<div>
  <!-- logged in/out links -->
  <% if logged_in? %>
  <%= link_to "Start a story", new_story_path %>
  <br>
  <%= link_to 'Log Out', logout_path %> |
  Hello <%= current_user.name %>
  <% else %>
  <%= link_to 'Log In', new_session_path %>
  or <%= link_to "Sign Up", new_user_path %>
  <br>
  <% end %>
  <hr>
</div>
